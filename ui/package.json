{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "yarn eslint .",
    "lint-fix": "yarn lint --fix",
    "test-all": "yarn lint && yarn type-check",
    "prepare": "cd .. && husky install ui/.husky"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "sh -c 'yarn type-check'",
      "yarn lint-fix"
    ]
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.34",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "eslint": "8.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "typescript": "4.6.4"
  }
}
